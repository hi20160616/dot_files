{
    "languageserver": {
	"golang": {
	    "command": "gopls",
	    "rootPatterns": ["go.work", "go.mod", ".vim/", ".git/", ".hg/"],
	    "filetypes": ["go"],
	    "initializationOptions": {
		"usePlaceholders": true
	    }
	}
    },
    "explorer.icon.enableNerdfont": true,
    "colors.enable": true,
    "clangd.path": "~/Git/dot_files/dot_config/coc/extensions/coc-clangd-data/install/14.0.3/clangd_14.0.3/bin/clangd",
    "workspace.ignoredFolders": [
    	"$HOME",
	"$HOME/.cargo/**",
	"$HOME/.rustup/**"
    ]
}
